{
  "version": "2.0",
  "logging": {
    "logLevel": {
      "default": "Trace"
    }
  },
  "extensions": {
    "http": {
      "routePrefix": "api/bonus-vacanze"
    },
    "durableTask": {
      "hubName": "%SLOT_TASK_HUBNAME%",
      "storageProvider": {
        "connectionStringName": "AzureWebJobsStorage"
      },
      "tracing": {
        "traceInputsAndOutputs": false,
        "traceReplayEvents": false
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[1.*, 2.0.0)"
  }
}
